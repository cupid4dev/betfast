{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "no-empty": 1,
    "no-irregular-whitespace": 1,
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ]
  }
}
